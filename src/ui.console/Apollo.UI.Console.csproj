    </ItemGroup>
    <Delete Files="@(GeneratedFiles)" />
  </Target>
  <!-- Just before we start building we'll generate the AssemblyInfo.XX.cs files-->
  <PropertyGroup>
    <BuildDependsOn>
            GenerateManifest;
            $(BuildDependsOn);
        </BuildDependsOn>
  </PropertyGroup>
  <Target Name="_GetSnPath">
    <GetFrameworkSdkPath>
      <Output TaskParameter="Path" PropertyName="WindowsSdkPath" />
    </GetFrameworkSdkPath>
    <PropertyGroup>
      <FileSNExe>$(WindowsSdkPath)bin\sn.exe</FileSNExe>
    </PropertyGroup>
    <!-- Sometimes theres nothing in the WindowsSdkPath dir and there's stuff in a deeper folder called 'NETFX 4.0 Tools'. -->
    <PropertyGroup>
      <FileSNExe Condition="!Exists($(FileSNExe))">$(WindowsSdkPath)bin\NETFX 4.0 Tools\sn.exe</FileSNExe>
    </PropertyGroup>
    <Message Text="sn.exe at: $(FileSNExe)" />
  </Target>
  <Target Name="GenerateManifest" DependsOnTargets="_GetSnPath">
    <PublicKeySignatureFromKeyFile SnExe="$(FileSNExe)" DirTemp="$(DirBuildTemp)" Input="$(SOFTWARE_SIGNING_KEY_PATH)">
      <Output TaskParameter="PublicKeyToken" PropertyName="PublicKeyTokenLocal" />
    </PublicKeySignatureFromKeyFile>
    <!-- Generate the version number file -->
    <ItemGroup>
      <ManifestTokens Include="Major">
        <ReplacementValue>$(VersionMajor)</ReplacementValue>
      </ManifestTokens>
      <ManifestTokens Include="Minor">
        <ReplacementValue>$(VersionMinor)</ReplacementValue>
      </ManifestTokens>
      <ManifestTokens Include="Build">
        <ReplacementValue>$(VersionBuild)</ReplacementValue>
      </ManifestTokens>
      <ManifestTokens Include="Revision">
        <ReplacementValue>$(VersionRevision)</ReplacementValue>
      </ManifestTokens>
      <ManifestTokens Include="ProjectName">
        <ReplacementValue>$(AssemblyName)</ReplacementValue>
      </ManifestTokens>
      <ManifestTokens Include="PublicKeyToken">
        <ReplacementValue>$(PublicKeyTokenLocal)</ReplacementValue>
      </ManifestTokens>
    </ItemGroup>
    <TemplateFile Template="$(FileTemplateApplicationManifest)" OutputFileName="$(FileApplicationManifest)" Tokens="@(ManifestTokens)" />
  </Target>
